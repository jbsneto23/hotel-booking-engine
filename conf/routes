# Rooms Routes
GET         /api/rooms                      controllers.RoomController.getAll()
GET         /api/rooms/:id                  controllers.RoomController.getById(id: Long)
POST        /api/rooms                      controllers.RoomController.create()
PUT         /api/rooms/:id                  controllers.RoomController.update(id: Long)
DELETE      /api/rooms/:id                  controllers.RoomController.delete(id: Long)

# Customer Routes
GET         /api/customers                      controllers.CustomerController.getAll()
GET         /api/customers/:id                  controllers.CustomerController.getById(id: Long)
POST        /api/customers                      controllers.CustomerController.create()
PUT         /api/customers/:id                  controllers.CustomerController.update(id: Long)
DELETE      /api/customers/:id                  controllers.CustomerController.delete(id: Long)

# Booking Routes
GET            /api/bookings                          controllers.BookingController.getAll()
GET            /api/bookings/occupancy                controllers.BookingController.occupancy()
GET            /api/bookings/availability             controllers.BookingController.availability()
GET            /api/bookings/:id                      controllers.BookingController.getById(id: Long)
POST           /api/bookings                          controllers.BookingController.create()
PUT            /api/bookings/:id                      controllers.BookingController.update(id: Long)
DELETE         /api/bookings/:id                      controllers.BookingController.delete(id: Long)
